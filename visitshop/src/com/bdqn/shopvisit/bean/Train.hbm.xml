<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.Train" table="train">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="title" type="string" length="32"></property>
		<property name="date" type="string" length="10"></property>
		<property name="location" type="string" length="255"></property>
		<property name="trainer" type="string" length="16"></property>
		<property name="trainId" type="string" length="16"></property>
	</class>
</hibernate-mapping>