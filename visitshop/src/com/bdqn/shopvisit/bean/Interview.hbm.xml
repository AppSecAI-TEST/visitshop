<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.Interview" table="interview">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="title" type="string" length="64"></property>
		<property name="personId" type="integer" length="11"></property>
		<property name="visitDate" type="string" length="10"></property>
		<property name="aim" type="string" length="255"></property>
		<property name="summary" type="string" length="255"></property>
		<property name="userId" type="string" length="16"></property>
	</class>
</hibernate-mapping>