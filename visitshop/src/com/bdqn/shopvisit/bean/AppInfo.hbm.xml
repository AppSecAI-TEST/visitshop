<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.AppInfo" table="appinfo">
		<id name="id" type="integer" column="id"  length="11">
			<generator class="native"></generator>
		</id>
		<property name="version" type="string" length="8"></property>
		<property name="publishDate" type="string" length="10"></property>
		<property name="updateInfo" type="string" length="512"></property>
		<property name="downloadUrl" type="string" length="255"></property>
	</class>
</hibernate-mapping>