<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.TrainData" table="traindata">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="score" type="string" length="6"></property>
		<property name="feedback" type="string" length="512"></property>
		<property name="imgPath" type="string" length="255"></property>
		<property name="imgName" type="string" length="512"></property>
		<property name="userId" type="string" length="16"></property>
		<property name="trainId" type="string" length="16"></property>
	</class>
</hibernate-mapping>