<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.FeedBack" table="feedback">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="userId" type="string" length="16"></property>
		<property name="date" type="string" length="10"></property>
		<property name="feedBack" type="string" length="512"></property>
	</class>
</hibernate-mapping>