<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.Info" table="info">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="title" type="string" length="128"></property>
		<property name="summary" type="string" length="255"></property>
		<property name="imgUrl" type="string" length="255"></property>
		<property name="detail" type="string" length="255"></property>
		<property name="type" type="int"></property>
	</class>
</hibernate-mapping>