<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.Shop" table="shop">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="name" type="string" length="128"></property>
		<property name="shopid" type="string" length="8"></property>
		<property name="location" type="string" length="255"></property>
	</class>
</hibernate-mapping>