<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.bdqn.shopvisit.bean.Customer" table="customer">
		<id name="id" type="integer" column="id" length="11">
			<generator class="native"></generator>
		</id>
		<property name="name" type="string" length="16"></property>
		<property name="phone" type="string" length="11"></property>
		<property name="company" type="string" length="255"></property>
		<property name="job" type="string" length="32"></property>

	</class>
</hibernate-mapping>